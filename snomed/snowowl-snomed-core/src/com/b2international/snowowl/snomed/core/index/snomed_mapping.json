                        "letter",
                        "digit",
                        "punctuation",
                        "symbol"
                    ]
                }
            },
            "filter": {
                "synonym": {
                    "type": "synonym",
                    "synonyms_path": "synonyms.txt"
                },
                "english_stemmer": {
                    "type": "stemmer",
                    "name": "english"
                },
                "edge_ngram": {
                    "type": "edgeNGram",
                    "min_gram": 1,
                    "max_gram": 20
                },
                "possessive": {
                    "type": "stemmer",
                    "name": "possessive_english"
                },
                "word_splitter": {
                    "type": "word_delimiter",
                    "split_on_case_change": "false",
                    "preserve_original": "true",
                    "stem_english_possessive": "false",
                    "type_table": [
                        ", => DIGIT",
                        ". => DIGIT"
                    ]
                },
                "unique_token": {
                    "type": "unique",
                    "only_on_same_position": "true"
                }
            }
        }
